{
  "plugins": [
    "add-react-displayname",
    "transform-decorators-legacy",
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "css": "./css",
          "components": "./components",
          "redactions": "./redactions",
          "utils": "./utils",
          "selectors": "./selectors",
          "static": "./static",
          "services": "./services"
        },
        "cwd": "babelrc"
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": ["css$", "scss$"]
      }
    ]
  ],
  "presets": [
    "next/babel"
  ],
  "ignore": [],
  "env": {
    "test": {
      "presets": [
        ["env", { "modules": "commonjs" }],
        "next/babel"
      ]
    }
  }
}
